    "typeRoots": ["./node_modules/@types", "./src/types"],
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "outDir": "dist",
    "esModuleInterop": true
  },
  "include": [
    "src/**/*.ts",
    "src/types/*.d.ts"
  ],
  "exclude": [
    "./node_modules"
  ],
}
